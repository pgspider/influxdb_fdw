--Testcase 14:
CREATE EXTENSION influxdb_fdw;
--Testcase 15:
CREATE SERVER server1 FOREIGN DATA WRAPPER influxdb_fdw OPTIONS
(dbname 'mydb', host :INFLUXDB_HOST, port :INFLUXDB_PORT);
